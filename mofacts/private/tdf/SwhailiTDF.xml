<tutor>
    <setspec>
        <lessonname>Learn Swahili!</lessonname>
        <stimulusfile>Swahilistims.xml</stimulusfile>
        <clustermodel>structuralpairs</clustermodel>
        <clustersize>1</clustersize>
        <shuffleclusters>0-16</shuffleclusters>
        <lfparameter>0.80</lfparameter>
        <eeglogged>true</eeglogged>
        <isModeled>false</isModeled>
        <timeoutInSeconds>15</timeoutInSeconds>
    </setspec>
    
    <deliveryparams>
        <verticalalign>true</verticalalign>
        <model>simplepaired</model>
        <blocking>false</blocking>
        <usessound>false</usessound>
        <responsesound>false</responsesound>
        <forcespacing>true</forcespacing>
        <numchoices>0</numchoices>
        <drillsonly>true</drillsonly>
        <responses>answer</responses>
        <initialclustershuffle>true</initialclustershuffle>
        <caseinsensitive>true</caseinsensitive>
        <border>0</border>
        <skipstudy>false</skipstudy>
        <optimizeinline>
            <numdrills1>50</numdrills1>
            <param1>initbeta</param1>
            <step1>0.05</step1>
            <type1>correctness</type1>
            <numdrills2>40</numdrills2>
            <param2>latencyscalar</param2>
            <step2>0.03</step2>
            <type2>latency</type2>
        </optimizeinline>
        <imagestretch>true</imagestretch>
    </deliveryparams>
    <parameters>
        <studyweight>1</studyweight>
        <decayinit>-0.24</decayinit>
        <decayscalar>.2493</decayscalar>
        <correctweight>2.3138</correctweight>
        <fincorrectweight>1.4743</fincorrectweight>
        <incorrectweight>.1</incorrectweight>
        <threshold>-.6891</threshold>
        <minactnoise>.2767</minactnoise>
        <psychadjust>-.031</psychadjust>
        <latencyscalar>2.2194</latencyscalar>
        <fixedcost>1.6</fixedcost>
        <latencyupdate>0.5847</latencyupdate>
        <initbeta>0</initbeta>
        <betabins>1</betabins>
        <betabinrange>7</betabinrange>
        <failuresecs>6</failuresecs>
        <retentioninterval>30</retentioninterval>
        <minima>
            <decayinit>-.3</decayinit>
            <latencyscalar>1.2</latencyscalar>
        </minima>
        <maxima>
            <decayinit>-.1</decayinit>
            <latencyscalar>3.22</latencyscalar>
        </maxima>
    </parameters>
    
    <unit>
        <unitname>Swahili Pre-test</unitname>
        <unitinstructions>
            <![CDATA[
                Enter answer in the prompt box below each question.  Please attempt to answer all questions presented.
                Also, click on 
                <a href='https://www.google.com/'
                   target="_blank"
                >The Survey</a>
            ]]>
        </unitinstructions>
        
        <instructconfirm>false</instructconfirm>
        <messagescore>false</messagescore>
        <correctscore>1</correctscore>
        <incorrectscore>0</incorrectscore>  
        <deliveryparams>
            <purestudy>10000</purestudy>
            <readyprompt>0</readyprompt>
            <reviewstudy>10000</reviewstudy>
            <correctprompt>0</correctprompt>
            <drill>80000</drill>
        </deliveryparams>
        <allowquit>false</allowquit>
        <allowpause>false</allowpause>
        <unending>false</unending>
        <unitconfigscreen>false</unitconfigscreen>
        <unitendscreen>false</unitendscreen>
        
        <buttontrial>true</buttontrial>

        <assessmentsession>
            <conditiontemplatesbygroup>
            <groupnames>A</groupnames> 
            <clustersrepeated>1</clustersrepeated>
            <templatesrepeated>17</templatesrepeated>
            <group>0,f,d,0 0,f,d,0 0,f,d,0 0,f,d,0 0,f,d,0
                0,f,d,0 0,f,d,0 0,f,d,0 0,f,d,0 0,f,d,0
                0,f,d,0 0,f,d,0 0,f,d,0 0,f,d,0 0,f,d,0
                0,f,d,0 0,f,d,0</group>
            </conditiontemplatesbygroup>
            <initialpositions>
                A_1 A_2 A_3 A_4 A_5 A_6 A_7 A_8 A_9 A_10 A_11 A_12 A_13 A_14 
                A_15 A_16 A_17 
            </initialpositions>
            <randomizegroups>false</randomizegroups>
            <clusterlist>0-16</clusterlist>
            <assignrandomclusters>false</assignrandomclusters>
            <permutefinalresult>0-16</permutefinalresult>
        </assessmentsession>
    </unit>

    <unit>
        <unitname>Lock out test</unitname>
        <lockoutminutes>5</lockoutminutes>
        <unitinstructions>
            <![CDATA[
                Further progress is blocked until the sepecified time has elapsed
            ]]>
        </unitinstructions>
    </unit>

    <unit>
        <unitname>Swahili Test 2</unitname>
        <unitinstructions>
            Answer multiple choice questions.  Please attempt to answer all questions presented.
        </unitinstructions>
        
        <instructconfirm>false</instructconfirm>
        <messagescore>false</messagescore>
        <correctscore>1</correctscore>
        <incorrectscore>0</incorrectscore>  
